<h1> <%= @place["name"] %></h1>

<h3>Posts</h3>

<ul>
  <% for post in @posts %>
    <li><a href="/posts/<%= post["id"] %>"><%= post["title"] %> </a></li>
  <% end %>
</ul>

<p>
  <a href="/posts/new?place_id=<%= @place["id"] %>"> New Post</a>
</p>

<p>
  <a href="/places">Back to All Places</a>
</p>
